            </documentation>
            <input message="tns:getClientApplicationRequest"/>
            <output message="tns:getClientApplicationResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>
        
        <operation name="addCertificateToDeveloper">
