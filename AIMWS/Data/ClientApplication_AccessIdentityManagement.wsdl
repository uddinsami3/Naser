            </documentation>
            <input message="tns:updateServiceInformationRequest"/>
            <output message="tns:updateServiceInformationResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="getClientApplication">
