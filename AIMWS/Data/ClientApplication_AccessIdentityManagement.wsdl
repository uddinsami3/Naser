    <portType name="ClientApplication_AccessIdentityManagement">
        <operation name="registerClientApplication">
            <documentation>
                Associates the developer ID, based on identity propagation, with the application ID within a specific platform (environment).
            </documentation>
            <input message="tns:registerClientApplicationRequest"/>
            <output message="tns:registerClientApplicationResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="getClientApplicationServices">
            <documentation>
                Retrieves the services associated with a client application
            </documentation>
            <input message="tns:getClientApplicationServicesRequest"/>
            <output message="tns:getClientApplicationServicesResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="grantServiceAccess">
            <documentation>
                Associates a service to a client application
            </documentation>
            <input message="tns:grantServiceAccessRequest"/>
            <output message="tns:grantServiceAccessResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>
        
        <operation name="addCertificateToClientApplication">
            <documentation>
                Adds a certificate to a client application
            </documentation>
            <input message="tns:addCertificateToClientApplicationRequest"/>
            <output message="tns:addCertificateToClientApplicationResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="removeCertificateFromClientApplication">
            <documentation>
                Removes a certificate from a client application
            </documentation>
            <input message="tns:removeCertificateFromClientApplicationRequest"/>
            <output message="tns:removeCertificateFromClientApplicationResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="removeClientApplication">
            <documentation>
                Removes a client application for the repository
            </documentation>
            <input message="tns:removeClientApplicationRequest"/>
            <output message="tns:removeClientApplicationResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="getInternalUsers">
            <documentation>
                Gets a list of users based on a single search criteria.
                The list may be empty.
            </documentation>
            <input message="tns:getInternalUserRequest"/>
            <output message="tns:getInternalUsersResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="setClientApplicationStatus">
            <documentation>
                Sets the application status to be active or inactive
            </documentation>
            <input message="tns:setClientApplicationStatusRequest"/>
            <output message="tns:setClientApplicationStatusResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="createService">
            <documentation>
                Creates a service with a Service Type for a specific Platform
            </documentation>
            <input message="tns:createServiceRequest"/>
            <output message="tns:createServiceResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="deleteService">
