            </documentation>
            <input message="tns:addCertificateToDeveloperRequest"/>
            <output message="tns:addCertificateToDeveloperResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>
        
        <operation name="removeCertificateFromDeveloper">
            <documentation>
