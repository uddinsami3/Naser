            </documentation>
            <input message="tns:getServiceInfoRequest"/>
            <output message="tns:getServiceInfoResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="revokeServiceAccess">
            <documentation>
                Removes access to a service
            </documentation>
            <input message="tns:revokeServiceRequest"/>
            <output message="tns:revokeServiceResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="updateServiceInformation">
            <documentation>
