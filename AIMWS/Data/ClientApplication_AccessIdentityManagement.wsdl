            </documentation>
            <input message="tns:deleteServiceRequest"/>
            <output message="tns:deleteServiceResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>

        <operation name="getServiceInfo">
            <documentation>
