            </documentation>
            <input message="tns:removeCertificateFromDeveloperRequest"/>
            <output message="tns:removeCertificateFromDeveloperResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>
        
        <operation name="findDeveloperCertificate">
            <documentation>
                Removes Certificate from the Developer
            </documentation>
            <input message="tns:findDeveloperCertificateRequest"/>
            <output message="tns:findDeveloperCertificateResponse"/>
            <fault name="aimErrorResponse" message="tns:aimErrorResponse"/>
        </operation>
    </portType>

